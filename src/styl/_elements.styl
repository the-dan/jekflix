.img-rounded
    border-radius 50%

.collage
  /* Prevent vertical gaps */
  line-height: 0;

  -webkit-column-count: 5;
  -webkit-column-gap:   0px;
  -moz-column-count:    5;
  -moz-column-gap:      0px;
  column-count:         5;
  column-gap:           0px;


.collage img
  /* Just in case there are inline attributes */
  width: 100% !important;
  height: auto !important;